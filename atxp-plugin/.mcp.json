{
  "mcpServers": {
    "atxp-fetch": {
      "command": "npx",
      "args": [
        "-y",
        "@atxp/mcp-fetch@latest"
      ],
      "env": {
        "ATXP_CONNECTION_STRING": "${ATXP_CONNECTION_STRING:-}",
        "ATXP_PROXY_ENABLED": "true"
      }
    },
    "atxp-image": {
      "command": "npx",
